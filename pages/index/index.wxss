/**index.wxss**/
.swiper{
  height: 320rpx;
}
.swiper swiper-item image{
  width: 100%;
  height: 100%
}
.icon{
  /* flex-wrap: wrap; */
  /* margin-top: 10rpx; */
  flex-flow: row wrap;
  background:#f5f5f5;
  border-bottom: 1rpx solid #ddd;
}
.icon view view{
  width: 100rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  margin: 0 41rpx;
  margin-top: 10rpx;
  background: #fff;
  /* margin:auto; */
  /* text-align:center; */
  align-items: center;
  border:3rpx solid #ddd;
  /* border-right: 1rpx solid #ddd; */
  border-radius: 50%;
}
.icon view view image{
  /* margin: 20rpx 0; */
   padding: 20rpx 0;
  width: 60%;
  /* height: 130rpx; */
}
.icon view text{
  display: flex;
  font-size: 32rpx;
  color:#999;
  margin: 5rpx 0;
  justify-content: center;
  /* margin: 0 50rpx; */
}
.icon view view{
  text-align: center;
}
/* font图标 */
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}


/* font图标样式 */
.iconfont {
  font-family: "iconfont" !important;
  font-size: 52rpx !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  color: #333 !important;
  text-align: center;
  padding-top: 15rpx;
}
